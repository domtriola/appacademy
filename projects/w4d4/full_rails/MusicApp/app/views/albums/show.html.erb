<h2><%= @album.title %></h2>

<h3>Tracks</h3>

<ul>
  <% @album.tracks.each do |track| %>
    <li><%= link_to track.title, track_url(track) %></li>
    <%= button_to 'delete', track_url(track), method: :delete %>
    <br>
  <% end %>
</ul>

<br>
<br>

<%= link_to 'Add track', new_band_album_track_url(@album.band.id, @album.id) %>

<br>
<br>

<%= link_to 'Back to band', band_url(@album.band) %>
