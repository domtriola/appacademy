<h2>Register</h2>

<form action="<%= users_url %>" method="post">
  <%= auth_token %>

  <label for="email">Email</label><br>
  <input id="email" type="text" name="user[email]">

  <br>
  <br>

  <label for="password">Password</label><br>
  <input type="password" name="user[password]">

  <br>
  <br>

  <input type="submit">
</form>
